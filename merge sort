void merge(int *arr1, int size1, int *arr2, int size2, int *ans){
    int i , j ,k;
    while(i<size1 && j<size2){
        if(arr1[i] <= arr2[j]){
            arr[i] = ans[k];
            i++;
        }
        else(arr2[j]<=arr1[i]){
            arr2[j] = ans[k];
            j++;
        }
        k++;
    }
    while(i<size1){
         ans[k] = arr1[i];
         i++;
         k++;
    }
     while(j<size2){
         ans[k] = arr2[j];
         j++;
         k++;
}